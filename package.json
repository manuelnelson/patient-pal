{
  "name": "practice-pal",
  "version": "0.0.0",
  "description": "Health Practice Tool",
  "main": "dist",
  "scripts": {
    "watch:backend": "nodemon -w src/server --exec \"babel-node src/server --presets es2015,stage-0\"",
    "build:backend": "babel src/server --ignore '/client/',webpack,public -s -D -d dist --presets es2015,stage-0",
    "build:dev": "webpack --env=dev --config ./src/webpack/webpack.dev.js",
    "build:prod": "webpack --env=prod --config ./src/webpack/webpack.prod.js",
    "dev": "webpack-dev-server --config ./src/webpack/webpack.dev.js",
    "start": "node dist",
    "postinstall": "npm run -s build:prod",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register src/server/tests --recursive",
    "start:dev": "npm-run-all -p dev watch:backend"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelnelson/practice-pal.git"
  },
  "author": "Emmanuel Nelson <info@emmanuelnelson.com>",
  "license": "MIT",
